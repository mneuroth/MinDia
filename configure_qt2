if test "$qt_libraries" = "$x_libraries" || test -z "$qt_libraries"; then
 QT_LDFLAGS=""
else
 QT_LDFLAGS="-L$qt_libraries"
 all_libraries="$all_libraries $QT_LDFLAGS"
fi

   qt_bindirs=""
   for dir in $kde_qt_dirs; do
      qt_bindirs="$qt_bindirs $dir/bin $dir/src/moc"
   done
   qt_bindirs="$qt_bindirs /usr/bin /usr/X11R6/bin /usr/local/qt/bin"
   if test ! "$ac_qt_bindir" = "NO"; then
      qt_bindirs="$ac_qt_bindir $qt_bindirs"
   fi

   echo "$as_me:10979: checking for moc" >&5
echo $ECHO_N "checking for moc... $ECHO_C" >&6
   if test -n "$MOC"; then
        kde_cv_path="$MOC";
   else
        kde_cache=`echo moc | sed 'y%./+-%__p_%'`

        if eval "test \"\${kde_cv_path_$kde_cache+set}\" = set"; then
  echo $ECHO_N "(cached) $ECHO_C" >&6
else

        kde_cv_path="NONE"
	dirs="$qt_bindirs"
	kde_save_IFS=$IFS
	IFS=':'
	for dir in $PATH; do
	  dirs="$dirs $dir"
        done
	IFS=$kde_save_IFS

        for dir in $dirs; do
	  if test -x "$dir/moc"; then
	    if test -n ""
	    then
              evalstr="$dir/moc  2>&1 "
	      if eval $evalstr; then
                kde_cv_path="$dir/moc"
                break
	      fi
            else
		kde_cv_path="$dir/moc"
                break
	    fi
          fi
        done

        eval "kde_cv_path_$kde_cache=$kde_cv_path"

fi

      eval "kde_cv_path=\"`echo '$kde_cv_path_'$kde_cache`\""

   fi

   if test -z "$kde_cv_path" || test "$kde_cv_path" = NONE; then
      echo "$as_me:11024: result: not found" >&5
echo "${ECHO_T}not found" >&6

    { { echo "$as_me:11027: error: No Qt meta object compiler (moc) found!
Please check whether you installed Qt correctly.
You need to have a running moc binary.
configure tried to run $ac_cv_path_moc and the test didn't
succeed. If configure shouldn't have tried this one, set
the environment variable MOC to the right one before running
configure.
" >&5
echo "$as_me: error: No Qt meta object compiler (moc) found!
Please check whether you installed Qt correctly.
You need to have a running moc binary.
configure tried to run $ac_cv_path_moc and the test didn't
succeed. If configure shouldn't have tried this one, set
the environment variable MOC to the right one before running
configure.
" >&2;}
   { (exit 1); exit 1; }; }

   else
      echo "$as_me:11046: result: $kde_cv_path" >&5
echo "${ECHO_T}$kde_cv_path" >&6
      MOC=$kde_cv_path

   fi

   if test -z "$UIC_NOT_NEEDED"; then

   echo "$as_me:11054: checking for uic" >&5
echo $ECHO_N "checking for uic... $ECHO_C" >&6
   if test -n "$UIC"; then
        kde_cv_path="$UIC";
   else
        kde_cache=`echo uic | sed 'y%./+-%__p_%'`

        if eval "test \"\${kde_cv_path_$kde_cache+set}\" = set"; then
  echo $ECHO_N "(cached) $ECHO_C" >&6
else

        kde_cv_path="NONE"
	dirs="$qt_bindirs"
	kde_save_IFS=$IFS
	IFS=':'
	for dir in $PATH; do
	  dirs="$dirs $dir"
        done
	IFS=$kde_save_IFS

        for dir in $dirs; do
	  if test -x "$dir/uic"; then
	    if test -n ""
	    then
              evalstr="$dir/uic  2>&1 "
	      if eval $evalstr; then
                kde_cv_path="$dir/uic"
                break
	      fi
            else
		kde_cv_path="$dir/uic"
                break
	    fi
          fi
        done

        eval "kde_cv_path_$kde_cache=$kde_cv_path"

fi

      eval "kde_cv_path=\"`echo '$kde_cv_path_'$kde_cache`\""

   fi

   if test -z "$kde_cv_path" || test "$kde_cv_path" = NONE; then
      echo "$as_me:11099: result: not found" >&5
echo "${ECHO_T}not found" >&6
      UIC=""
   else
      echo "$as_me:11103: result: $kde_cv_path" >&5
echo "${ECHO_T}$kde_cv_path" >&6
      UIC=$kde_cv_path

   fi

     if test -z "$UIC" ; then

    { echo "$as_me:11111: WARNING: No Qt ui compiler (uic) found!
Please check whether you installed Qt correctly.
You need to have a running uic binary.
configure tried to run $ac_cv_path_uic and the test didn't
succeed. If configure shouldn't have tried this one, set
the environment variable UIC to the right one before running
configure.
" >&5
echo "$as_me: WARNING: No Qt ui compiler (uic) found!
Please check whether you installed Qt correctly.
You need to have a running uic binary.
configure tried to run $ac_cv_path_uic and the test didn't
succeed. If configure shouldn't have tried this one, set
the environment variable UIC to the right one before running
